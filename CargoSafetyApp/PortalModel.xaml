                                    <DoubleAnimation Storyboard.TargetName="CameraAngle"  Storyboard.TargetProperty="(AxisAngleRotation3D.Angle)"    To="0" Duration="{StaticResource ViewDuration}" />
									<Vector3DAnimation  Storyboard.TargetName="CameraAngle"  Storyboard.TargetProperty="(AxisAngleRotation3D.Axis)"  To="0,1,0"  Duration="{StaticResource ViewDuration}" />
                                    <DoubleAnimation Storyboard.TargetName="CameraOffset" Storyboard.TargetProperty="(TranslateTransform3D.OffsetX)" To="0" Duration="{StaticResource ViewDuration}" />
                                    <DoubleAnimation Storyboard.TargetName="CameraOffset" Storyboard.TargetProperty="(TranslateTransform3D.OffsetY)" To="0" Duration="{StaticResource ViewDuration}" />
                                    <DoubleAnimation Storyboard.TargetName="CameraOffset" Storyboard.TargetProperty="(TranslateTransform3D.OffsetZ)" To="0" Duration="{StaticResource ViewDuration}" />
                                </Storyboard>
                            </BeginStoryboard>
                        </EventTrigger>
