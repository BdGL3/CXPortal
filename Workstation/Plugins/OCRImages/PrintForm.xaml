<UserControl x:Class="L3.Cargo.Workstation.Plugins.OCRImages.PrintForm"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             Height="Auto" Width="Auto">
    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/L3.Cargo.Common;component/ResourceDictionary.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </UserControl.Resources>
    <DockPanel>
        <GroupBox Margin="5" Header="{Binding Path=OCRImagesPrintOptions, Source={StaticResource Resources}}">
            <ListBox SelectionMode="Multiple" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Margin="2" SelectionChanged="OCRImagesListBox_SelectionChanged" ItemsSource="{Binding}" DisplayMemberPath="FullName" Name="OCRImages_ListBox" />
        </GroupBox>
    </DockPanel>
</UserControl>
