    <tagGroup name="ScanGroup" updaterate="100">
      <tag name="SCAN_AREA_CLEAR" type="Control" detail="Indicates site is clear and scans can be started." />

      <!-- CALIBRATION_SCAN: this bit is set high to indicate that the next scan line should be a
      calibration scan. This bit shall stay high until the calibration scan has been completed.
      This bit is set low to indicate that the next scan should be a regular scan. This bit shall
      stay low as long as a calibration scan has not been requested. This bit shall be set high by
      the ClearView Dashboard and set low by the PLC. -->
      <tag name="CALIBRATION_SCAN" type="Control" detail="Indicates the next scan should be a calibration." />

      <tag name="PORTAL_OBJECT_FOUND" type="Information" detail="Indicates that the portal sensor has found an object." />
      <tag name="SCAN_MULTIPLE_OBJECTS" type="Information" detail="Indicates that the system should run in a single scan mode or a multiple scan mode." />

      <!-- SCAN_STATE: this unsigned integer is set by the PLC to announce the scan state. -->
      <tag name="SCAN_STATE" type="Information" detail="Gets the status of the scan system." >
        <tagValue value="0" type="Scan Ready" />
        <tagValue value="1" type="Scan Not Ready" />
        <tagValue value="2" type="Scanning" />
      </tag>

      <!-- CALIBRATION_STATE: this unsigned integer is set by the PLC to announce the calibration state. -->
      <tag name="CALIBRATION_STATE" type="Information" detail="Gets the status of the calibration." >
        <tagValue value="0" type="Calibration ready to be performed" />
        <tagValue value="1" type="Calibration not ready to be performed" />
        <tagValue value="2" type="Calibration failed" />
        <tagValue value="3" type="Calibrating high energy" />
        <tagValue value="4" type="Calibration high energy complete" />
        <tagValue value="5" type="Calibrating low energy" />
        <tagValue value="6" type="Calibration low energy complete" />
        <tagValue value="7" type="Calibrating dual energy" />
        <tagValue value="8" type="Calibrating dual energy complete" />
        <tagValue value="9" type="Calibration completed" />
        <tagValue value="10" type="Calibrating dark data" />
        <tagValue value="11" type="Calibration dark data complete" />
      </tag>

      <tag name="SCAN_DRIVE_DIRECTION" type="Information" detail="Gets the direction of the scan." />
      <tag name="SCAN_DRIVE_SELECTED_SPEED" type="Information" detail="Gets the selected speed of the scan." />
      <tag name="SCAN_DRIVE_STATE" type="Information" detail="Gets the status of the poclain drive." />
      <tag name="SCAN_DRIVE_SIGN_OF_LIFE" type="Status" detail="Gets the status of the poclain drive.">
        <tagValue value="0" type="Error" />
        <tagValue value="1" type="Clear" />
      </tag>

      <!--
      <tag name="SCAN_DRIVE_POWER_STATUS" type="Status" detail="Gets the status of the power state of the poclain drive.">
        <tagValue value="0" type="Error" />
        <tagValue value="1" type="Clear" />
      </tag>
      -->
      <tag name="SCAN_DRIVE_HAND_BRAKE" type="Status" detail="Gets the status of the hand brake.">
        <tagValue value="0" type="Clear" />
        <tagValue value="1" type="Warning" />
      </tag>
      <tag name="SCAN_DRIVE_SPEED_FAILURE" type="Status" detail="Gets the status of the of the poclain drive speed information.">
        <tagValue value="0" type="Clear" />
        <tagValue value="1" type="Error" />
        <tagValue value="2" type="Error" />
      </tag>
      <tag name="SCAN_DRIVE_HYDRAULIC_OIL_TEMPERATURE" type="Information" detail="Gets the temperature of the hydraulic oil." />
      <tag name="SCAN_DRIVE_HYDRAULIC_OIL_LEVEL" type="Information" detail="Gets the level of the hydraulic oil." />
      <tag name="SCAN_DRIVE_REALTIME_SPEED" type="Information" detail="Gets the realtime speed of the scan." />
      <tag name="SITE_READY_NEXT_SCAN" type="Information" detail="Allows the driver to click the allow scan button." />
